{
  "name": "docs",
  "root": "docs",
  "sourceRoot": "docs/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["build"],
      "options": {
        "command": "npm run build",
        "cwd": "docs"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run start",
        "cwd": "docs"
      }
    }
  },
  "tags": ["lang:js"]
}
